import datetime
import os

@returns(bool)
def process(inputs):

  vid_width=str(2048)
  vid_height=str(2048)
  vid_rate=str(30)
  pipeName=inputs.Item1.Item2
  fname = inputs.Item2 +pipeName[-15:] + '.avi'
  cam_id=str(0)
  codec_q=str(5)

  #pipeName=pipeName.replace('\\','\\\\')
  print 'using pipe',pipeName
  cmd='c:\\ffmpeg\\bin\\ffmpeg.exe -y -f rawvideo -vcodec rawvideo -s '+vid_width+'x'+vid_height+' -r '+vid_rate+' -pix_fmt gray -i '+pipeName+' -q:v '+codec_q+' -vcodec mpeg4 -vtag xvid '+ fname
  #sp.Popen(cmd)
  os.system(cmd)
  return True</q3:Script>
